{
  "name": "doomd-sk-windi-template",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "dev:only": "npm run setver && svelte-kit dev",
    "build:only": "npm run setver && svelte-kit build",
    "dev": "npm run dev:only",
    "build": "npm run build:only",
    "insertvars": "node npm_scripts/insert_vars.js",
    "setver": "genversion --es6 src/lib/config/version.ts",
    "postsetver": "npm run insertvars",
    "getver": "node -p process.env.npm_package_version",
    "preversion": "git fetch --all && git status && git switch develop",
    "version": "npm run getver && npm run setver && git add --all",
    "postversion": "git push --follow-tags origin develop",
    "rel": "npm version --force",
    "beta": "npm version --force",
    "postbeta": "cli-confirm \"Do you want to still want to deploy to master?\" && npm run postrel",
    "postrel": "git checkout master && git merge --ff-only develop && git push --set-upstream origin master && git checkout develop"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "genversion": "^3.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.10.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.3",
    "vite-plugin-windicss": "^1.5.4",
    "cli-confirm": "^1.0.1",
    "windicss": "^3.3.0"
  },
  "archive": {
    "preinsertvars": "tsc --resolveJsonModule npm_scripts/insert_vars.ts",
    "insertvars": "node --loader ts-node/esm npm_scripts/insert_vars.ts",
    "enquirer": "^2.3.6",
    "somethingidontuseanymore": "but i didn't want to delete just yet..."
  }
}
